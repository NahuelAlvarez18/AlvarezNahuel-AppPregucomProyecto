<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_Infor_Lv5">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondo_app_definitivo_prin"
        tools:context=".Activity_Hardware">

        <CheckBox
            android:id="@+id/checkBox51"
            android:layout_width="179dp"
            android:layout_height="40dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="40dp"
            android:buttonTint="#FFFFFF"
            android:onClick="Checkbox51"
            android:text="@string/CB55_InforUSB"
            android:textColor="#FFFFFF"
            android:textColorHighlight="@color/colorAccent"
            android:textColorHint="#FFFFFF"
            android:textCursorDrawable="@color/design_default_color_primary"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <CheckBox
            android:id="@+id/checkBox52"
            android:layout_width="179dp"
            android:layout_height="40dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="32dp"
            android:buttonTint="#FFFFFF"
            android:onClick="Checkbox52"
            android:text="@string/CB33_InforCMD"
            android:textColor="#FFFFFF"
            android:textColorHighlight="@color/colorAccent"
            android:textColorHint="#FFFFFF"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBox51" />

        <CheckBox
            android:id="@+id/checkBox53"
            android:layout_width="179dp"
            android:layout_height="40dp"
            android:layout_marginStart="48dp"
            android:layout_marginTop="32dp"
            android:buttonTint="#FFFFFF"
            android:onClick="Checkbox53"
            android:text="@string/CB55_InforPerifericos"
            android:textColor="#FFFFFF"
            android:textColorHighlight="@color/colorAccent"
            android:textColorHint="#FFFFFF"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/checkBox52" />

        <TextView
            android:id="@+id/tv_color2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#773333"
            android:padding="12sp"
            android:text="@string/Color_padding"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/Iv_6"
            android:layout_width="216dp"
            android:layout_height="346dp"
            android:layout_marginBottom="392dp"
            android:onClick="Checkbox22"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/circuloicono___copia" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="371dp"
            android:layout_height="172dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/foto_text_view"
            android:padding="10sp"
            android:text="@string/tv_Inforlv5"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@android:color/white"
            android:textIsSelectable="false"
            android:textSize="24sp"
            android:textStyle="bold"
            android:typeface="monospace"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_color2" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>